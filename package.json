{
  "name": "react-bdmap",
  "version": "0.1.0",
  "description": "Baidu Map Components for React",
  "keywords": [
    "react",
    "bdmap",
    "baidu",
    "map",
    "web"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "es/index.d.ts",
  "scripts": {
    "prebuild": "rimraf lib es",
    "build:es6": "tsc -d --outDir ./lib --module CommonJS ",
    "postbuild:es6": "cpx \"src/**/!(example)/*.+(css|jpg|jpeg|gif|png)\" ./lib",
    "postbuild:es6module": "cpx \"src/**/!(example)/*.+(css|jpg|jpeg|gif|png)\" ./es",
    "build:es6module": "tsc -d --outDir ./es",
    "build": "npm run build:es6 && npm run build:es6module",
    "build:doc": "styleguidist build",
    "start:doc": "styleguidist server"
  },
  "files": [
    "lib",
    "es"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/carney520/react-bdmap.git"
  },
  "author": "carney520",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/carney520/react-bdmap/issues"
  },
  "homepage": "https://github.com/carney520/react-bdmap#readme",
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "tabWidth": 2,
    "printWidth": 120,
    "trailingComma": "all",
    "arrowParens": "avoid",
    "overrides": [
      {
        "files": "*.md",
        "options": {
          "printWidth": 80
        }
      }
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/baidumap-web-sdk": "^2.0.3",
    "@types/lodash": "^4.14.118",
    "@types/react": "^16.7.6",
    "@types/react-dom": "^16.0.9",
    "@types/shallowequal": "^0.2.3",
    "cpx": "^1.5.0",
    "husky": "^1.2.0",
    "parcel": "^1.10.3",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0",
    "react": "^16.6.3",
    "react-docgen-typescript": "^1.12.2",
    "react-dom": "^16.6.3",
    "react-styleguidist": "^8.0.6",
    "rimraf": "^2.6.2",
    "ts-loader": "^5.3.0",
    "typescript": "^3.1.6",
    "webpack": "^4.26.0"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "shallowequal": "^1.1.0",
    "tslib": "^1.9.3"
  }
}
